package javaapplication4;

import java.util.Scanner;

public class series2 {
    public static void main(String[] args) {
        int n;int sum=1;
        Scanner input=new Scanner(System.in);
        System.out.println("Enter series number");
        n=input.nextInt();
        for (int i = 1; i <=n; i++) {
            sum=sum*i;
            System.out.print(i +" ");
              
        }
        System.out.println();
        System.out.println(sum);
    }
}
