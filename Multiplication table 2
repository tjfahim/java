package javaapplication4;

import java.util.Scanner;

public class MultiplicationTable2 {
    public static void main(String[] args) {
        Scanner input=new Scanner(System.in);
        System.out.println("Enter 1st number for multiple table");
        int m=input.nextInt();
        System.out.println("Enter 2nd number for multiple table");
        int n=input.nextInt();
        for (int i = m; i <= n; i++) {
            for (int j = 1; j < 10; j++) {
                System.out.print((i*j) +" = "+ i +"*"+j);
                System.out.println("");
               
                
                 }
            System.out.println("");
        }
    }
}
