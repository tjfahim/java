
import java.util.Scanner;

class Array{
public static void main (String args[]){
    int n;
    Scanner input=new Scanner(System.in);
    System.out.println("Enter an int number");
    n=input.nextInt();
    for(int i=n;i>=0;i--){
    for(int j=0;j<=i;j++){
    System.out.print(j +" ");
    }
    System.out.println(" ");
    }
}
}
